#!/bin/bash
source ~/.bash/distro
source ~/.bash/env
source ~/.bash/aliases
source ~/.bash/functions

# STARSHIP PROMPT
# install with `curl -sS https://starship.rs/install.sh | sh`
# or `cargo install starship`
eval "$(starship init bash)"

# Autocomplete pandoc options
eval "$(pandoc --bash-completion)"

# FZF
source $FZFDIR/key-bindings.bash # manjango
source $FZFDIR/completion.bash # manjango

source /usr/share/doc/pkgfile/command-not-found.bash # manjango

neofetch
